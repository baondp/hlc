{
    "manifest_version": 2,

    "name": "HLC",
    "description": "Make notes and marks on your web page.",
    "version": "1.0.0",

    "permissions": [
        "activeTab",
        "http://localhost:23333/"
    ],
    "browser_action": {
        "default_icon": "icon/pen-50.png",
        "default_popup": "index.html"
    },
    "content_scripts": [
        {
            "matches": [ "<all_urls>"],
            "css": [ ],
            "js": [ 
                "dist/hlc.inject.js"
            ]
        }
    ]

}